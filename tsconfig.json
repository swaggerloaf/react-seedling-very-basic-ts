{
  "include": ["src/index.tsx"],
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "esModuleInterop": true,
    "types": ["react/next", "react-dom/next"]
  }
}
